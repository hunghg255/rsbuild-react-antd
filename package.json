{
  "name": "rsbuild-react-antd",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "rsbuild dev --open",
    "build": "rsbuild build",
    "buildDev": "rsbuild build",
    "buildProd": "rsbuild build",
    "preview": "rsbuild preview",
    "start": "npm run build && rsbuild preview",
    "lint": "eslint src --quiet --flag unstable_ts_config",
    "lint-fix": "eslint src --fix",
    "analyze": "RSDOCTOR=true npm run build",
    "verify-commit": "verify-commit-msg",
    "prepare": "git-scm-hooks",
    "release": "bumpp -r",
    "gen-icon": "csvgtocss"
  },
  "dependencies": {
    "ahooks": "^3.8.4",
    "antd": "^5.23.2",
    "brainless-token-manager": "^1.3.3",
    "classnames": "^2.5.1",
    "dayjs": "^1.11.13",
    "i18next": "^23.16.8",
    "i18next-browser-languagedetector": "^7.2.2",
    "i18next-http-backend": "^2.7.1",
    "jotai": "^1.13.1",
    "jotai-devtools": "^0.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet": "^6.1.0",
    "react-i18next": "^12.3.1",
    "react-router-dom": "^6.28.2",
    "rspack-plugin-dotenv": "^0.0.3",
    "umi-request": "^1.4.0"
  },
  "devDependencies": {
    "@hunghg255/eslint-config": "^0.0.15",
    "@rsbuild/core": "^1.2.0",
    "@rsbuild/plugin-react": "^1.1.0",
    "@rsbuild/plugin-sass": "1.2.0",
    "@rsdoctor/rspack-plugin": "^0.4.13",
    "@types/node": "^20.17.14",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@types/react-helmet": "^6.1.11",
    "antd-form-devtools": "^1.0.2",
    "bumpp": "^9.10.2",
    "csvgtocss": "^0.1.3",
    "eslint": "^9.17.0",
    "eslint-plugin-tailwindcss": "^3.18.0",
    "git-scm-hooks": "^0.0.6",
    "postcss": "^8.5.1",
    "rsbuild-plugin-console-debug": "0.0.9",
    "rsbuild-plugin-print": "^0.0.1",
    "rsbuild-plugin-react-inspector": "^0.0.13",
    "sass": "^1.83.4",
    "source-map-explorer": "^2.5.3",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.3",
    "verify-commit-msg": "^0.0.11"
  },
  "browserslist": [
    ">0.3%",
    "not ie 11",
    "not dead",
    "not op_mini all"
  ],
  "git-hooks": {
    "pre-commit": "npm run lint",
    "commit-msg": "npm run verify-commit"
  },
  "packageManager": "pnpm@9.15.4"
}
