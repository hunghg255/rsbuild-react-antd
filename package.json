{
  "name": "rsbuild-react-antd",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "dev": "rsbuild dev --open",
    "build": "rsbuild build",
    "buildDev": "rsbuild build",
    "buildProd": "rsbuild build",
    "preview": "rsbuild preview",
    "start": "npm run build && rsbuild preview",
    "lint": "eslint src --quiet",
    "lint-fix": "eslint src --fix",
    "format": "prettier --write \"./src/**/*.{ts,tsx,json}\"",
    "analyze": "RSDOCTOR=true npm run build",
    "verify-commit": "verify-commit-msg",
    "prepare": "git-scm-hooks",
    "release": "bumpp -r",
    "gen-icon": "csvgtocss"
  },
  "dependencies": {
    "ahooks": "^3.8.1",
    "antd": "^5.21.1",
    "brainless-token-manager": "^1.3.3",
    "classnames": "^2.5.1",
    "dayjs": "^1.11.13",
    "i18next": "^23.15.1",
    "i18next-browser-languagedetector": "^7.2.1",
    "i18next-http-backend": "^2.6.1",
    "jotai": "^1.13.1",
    "jotai-devtools": "^0.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet": "^6.1.0",
    "react-i18next": "^12.3.1",
    "react-router-dom": "^6.26.2",
    "rspack-plugin-dotenv": "^0.0.3",
    "umi-request": "^1.4.0"
  },
  "devDependencies": {
    "@eslint/compat": "^1.1.1",
    "@eslint/js": "^9.10.0",
    "@hunghg255/eslint-config-react": "^0.0.16",
    "@rsbuild/core": "^1.0.1",
    "@rsbuild/plugin-react": "^1.0.1",
    "@rsbuild/plugin-sass": "1.0.1-beta.8",
    "@rsdoctor/rspack-plugin": "^0.4.4",
    "@types/node": "^20.16.9",
    "@types/react": "^18.3.9",
    "@types/react-dom": "^18.3.0",
    "@types/react-helmet": "^6.1.11",
    "antd-form-devtools": "^1.0.2",
    "bumpp": "^9.5.2",
    "csvgtocss": "^0.1.1",
    "eslint": "^9.10.0",
    "eslint-plugin-react": "^7.35.2",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "git-scm-hooks": "^0.0.6",
    "globals": "^15.9.0",
    "prettier": "^2.8.8",
    "rsbuild-plugin-console-debug": "0.0.9",
    "rsbuild-plugin-print": "^0.0.1",
    "rsbuild-plugin-react-inspector": "^0.0.13",
    "sass": "^1.79.3",
    "source-map-explorer": "^2.5.3",
    "typescript": "^5.5.2",
    "typescript-eslint": "^8.7.0",
    "verify-commit-msg": "^0.0.11"
  },
  "browserslist": [
    ">0.3%",
    "not ie 11",
    "not dead",
    "not op_mini all"
  ],
  "git-hooks": {
    "pre-commit": "npm run lint",
    "commit-msg": "npm run verify-commit"
  },
  "packageManager": "pnpm@9.11.0+sha512.0a203ffaed5a3f63242cd064c8fb5892366c103e328079318f78062f24ea8c9d50bc6a47aa3567cabefd824d170e78fa2745ed1f16b132e16436146b7688f19b"
