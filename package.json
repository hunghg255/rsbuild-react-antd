{
  "name": "rsbuild-react-antd",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "dev": "rsbuild dev",
    "build": "npm run lint && rsbuild build",
    "buildDev": "npm run lint && rsbuild build",
    "buildProd": "npm run lint && rsbuild build",
    "preview": "rsbuild preview",
    "start": "npm run build && rsbuild preview",
    "lint": "eslint --ext .ts,.tsx src --color",
    "format": "prettier --write \"./src/**/*.{ts,tsx,json}\"",
    "gen-icon": "csvg-to-font",
    "analyze": "RSDOCTOR=true npm run build",
    "verify-commit": "verify-commit-msg",
    "postinstall": "git-scm-hooks",
    "release": "bumpp -r"
  },
  "dependencies": {
    "ahooks": "^3.7.8",
    "antd": "^5.12.8",
    "brainless-token-manager": "^1.3.3",
    "classnames": "^2.5.1",
    "dayjs": "^1.11.10",
    "i18next": "^23.7.16",
    "i18next-browser-languagedetector": "^7.2.0",
    "i18next-http-backend": "^2.4.2",
    "jotai": "^1.13.1",
    "jotai-devtools": "^0.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-i18next": "^12.3.1",
    "react-router-dom": "^6.21.1",
    "rspack-plugin-dotenv": "^0.0.3",
    "umi-request": "^1.4.0"
  },
  "devDependencies": {
    "@hunghg255/eslint-config-react": "^0.0.16",
    "@rsbuild/core": "^0.3.0",
    "@rsbuild/plugin-react": "^0.3.0",
    "@rsdoctor/rspack-plugin": "^0.1.0",
    "@total-typescript/ts-reset": "^0.5.1",
    "@types/node": "^20.10.8",
    "@types/react": "^18.2.47",
    "@types/react-dom": "^18.2.18",
    "@types/react-helmet": "^6.1.11",
    "@vitejs/plugin-react": "^4.2.1",
    "antd-form-devtools": "^1.0.2",
    "bumpp": "^9.2.1",
    "csvg-to-font": "^1.0.20",
    "eslint": "^8.56.0",
    "git-scm-hooks": "^0.0.6",
    "prettier": "^2.8.8",
    "rsbuild-plugin-print": "^0.0.1",
    "sass": "^1.69.7",
    "source-map-explorer": "^2.5.3",
    "typescript": "^4.9.5",
    "verify-commit-msg": "^0.0.11"
  },
  "browserslist": [
    ">0.3%",
    "not ie 11",
    "not dead",
    "not op_mini all"
  ],
  "git-hooks": {
    "pre-commit": "npm run lint",
    "commit-msg": "npm run verify-commit"
  }
}
